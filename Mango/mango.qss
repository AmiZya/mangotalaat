    border-width: 4px;
    border-image: url(:/images/normal_button.png) 4 4 4 4 stretch stretch;
    min-width: 100px;
    min-height: 100px;
    text-align: center;
    color: black;
}

#propertyWidget QToolButton:hover
{
    border-width: 4px;
    border-image: url(:/images/hover_button.png) 4 4 4 4 stretch stretch;
    min-width: 100px;
    min-height: 100px;
}

#propertyWidget QToolButton:pressed, #categoryWidget QToolButton:checked
