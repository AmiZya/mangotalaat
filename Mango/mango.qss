 
#categoryWidget QToolButton
{
    border-width: 4px;
    border-image: url(:/images/normal_button.png) 4 4 4 4 stretch stretch;
    min-width: 128px;
    min-height: 128px;
    text-align: center;
    color: black;
}

#categoryWidget QToolButton:hover
{
    border-width: 4px;
    border-image: url(:/images/hover_button.png) 4 4 4 4 stretch stretch;
    min-width: 128px;
    min-height: 128px;
}

#categoryWidget QToolButton:pressed, #categoryWidget QToolButton:checked
{
    border-width: 4px;
    border-image: url(:/images/pressed_button.png) 4 4 4 4 stretch stretch;
    min-width: 128px;
    min-height: 128px;
}


#propertyWidget QToolButton
